{
  "tags": {
    "allowUnknownTags": false
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "./dist/docs/gen",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": "false"
    }
  },
  "source": {
    "includePattern": "server/api/**/*.(service|model).js"
  }
}
